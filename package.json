{
    "name": "frida-clockwork",
    "version": "1.1.0",
    "description": "Collection oof various frida toys",
    "type": "module",
    "scripts": {
        "clean": "rm -rf packages/*/*.tsbuildinfo && rm -rf packages/*/dist && rm -rf node_modules/",
        "prepublish": "npm run tsc",
        "tsc": "npx tsc --build",
        "webpack": "npx webpack --config webpack.config.js",
        "watch:tsc": "npx tsc -b -w",
        "watch:webpack": "npx webpack --config webpack.config.js --watch",
        "attach": "run() { frida -U \"$1\" -l ./script.js -o ./session.txt; }; run",
        "spawn": "run() { frida -U -f \"$1\" -l ./script.js -o ./session.txt ; }; run"
    },
    "keywords": [
        "frida"
    ],
    "author": "Arsylk",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Arsylk/frida-clockwork/issues"
    },
    "homepage": "https://github.com/Arsylk/frida-clockwork",
    "dependencies": {
        "copy-webpack-plugin": "^12.0.2",
        "frida-compile": "^16.4.1",
        "frida-panic": "^3.0.0",
        "il2cpp-hooker": "^1.0.7"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@types/frida-gum": "^18.7.1",
        "frida-il2cpp-bridge": "^0.9.1",
        "ts-loader": "^9.5.1",
        "typescript": "^5.8.3",
        "webpack": "^5.95.0",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "^6.0.1"
    },
    "workspaces": [
        "packages/logging",
        "packages/common",
        "packages/hooks",
        "packages/network",
        "packages/native",
        "packages/jnitrace",
        "packages/cocos2dx",
        "packages/unity",
        "packages/anticloak",
        "packages/dump",
        "packages/cmodules"
    ]
}
